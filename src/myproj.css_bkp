  }


/*Media screen for Mobile */
@media screen and (max-width: 768px) and (min-width: 300px) {
  .main-nav {
    display: none;
  }
  .container {
      display: flex;
      flex-direction: column;
      justify-content: flex-start;
      border: rgb(4, 3, 3) solid 10px;
  }

  .main-content {
    display: flex;
    flex: 1;
    flex-direction:column;
    padding-top: 25px;
    padding-bottom: 700px;
    overflow: auto;
    height: 100vh;
    background-image: url(../docs/backgroundimage.jpeg);
    
}
.project-images {
  display: flex;
  padding-left: 30px;
  
  
  }
.prjimg{
    height: 80px;
    width: 80px;
      }
.prjimg1{
        height: 80px;
        width: 130px;
          }
.prjimg2{
            height: 80px;
            width: 80px;
              }      
}

/*Media screen for tablets*/
