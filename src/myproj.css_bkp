/*Container*/
.container {
    display: flex;
    flex-direction: row wrap;
    justify-content: flex-start;
    border: rgb(4, 3, 3) solid 10px;
}
/*Navigation Bar*/
.main-nav{
    display: flex;
    padding-right: 20px;
    background-color:rgb(119, 150, 197);;
    align-items: center;
}
.nav-item {
    list-style: none;
    margin-bottom: 50px;
}
.nav-item a {
    text-decoration: none;
}

.nav-item:hover{
    transform: scale(1.2);
}
/*Main content*/
.main-content {
    display: flex;
    flex: 1;
    flex-direction:column;
    padding-top: 25px;
    overflow: auto;
    background-image: url(../docs/backgroundimage.jpeg);

}

.myprojects{        
    font-size: 40px;
    color: rgb(122, 24, 24);  
    text-align: center; 
    box-shadow: 0 0 20px 0  blue; 
}

